<Config>
  <LogLevel>{{ getenv "PROWLARR__LOG_LEVEL" | default "info" }}</LogLevel>
  <UrlBase>{{ getenv "PROWLARR__URL_BASE" | default "" }}</UrlBase>
  <Branch>{{ getenv "PROWLARR__BRANCH" | default "master" }}</Branch>
  <ApiKey>{{ getenv "PROWLARR__API_KEY" | default "changemetosomethingalittlelongertrythisandodit" }}</ApiKey>
  <AuthenticationMethod>{{ getenv "PROWLARR__AUTHENTICATION_METHOD" | default "Forms" }}</AuthenticationMethod>
  <AuthenticationRequired>{{ getenv "PROWLARR__AUTHENTICATION_REQUIRED" | default "DisabledForLocalAddresses" }}</AuthenticationRequired>
  <InstanceName>{{ getenv "PROWLARR__INSTANCE_NAME" | default "Prowlarr" }}</InstanceName>
  <Port>{{ getenv "PROWLARR__PORT" | default "9696" }}</Port>
  <BindAddress>{{ getenv "PROWLARR__BIND_ADDRESS" | default "*" }}</BindAddress>
  <LaunchBrowser>{{ getenv "PROWLARR__LAUNCH_BROWSER" | default "False" }}</LaunchBrowser>
  <EnableSsl>{{ getenv "PROWLARR__ENABLE_SSL" | default "False" }}</EnableSsl>
  <SslPort>{{ getenv "PROWLARR__SSL_PORT" | default "6969" }}</SslPort>
  <SslCertPath>{{ getenv "PROWLARR__SSL_CERT_PATH" | default "" }}</SslCertPath>
  <SslCertPassword>{{ getenv "PROWLARR__SSL_CERT_PASSWORD" | default "" }}</SslCertPassword>
  <UpdateMechanism>{{ getenv "PROWLARR__UPDATE_MECHANISM" | default "Docker" }}</UpdateMechanism>
  <AnalyticsEnabled>{{ getenv "PROWLARR__ANALYTICS_ENABLED" | default "False" }}</AnalyticsEnabled>
  <Theme>{{ getenv "PROWLARR__THEME" | default "auto" }}</Theme>
</Config>
